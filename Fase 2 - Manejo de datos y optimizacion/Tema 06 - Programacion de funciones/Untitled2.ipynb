    "import random\n",
    "li=[]\n",
    "con=0\n",
    "n=int(input(\"Â¿Cuantos valores quieres en tu lista?  \"))\n",
    "num=random.randint(0,9)\n",
    "li.append(num)\n",
    "while len(li)!=n:\n",
    "    num=random.randint(0,9)\n",
    "    for l2 in li:\n",
    "        if num==l2:\n",
    "            con+=1\n",
    "    if con==0:\n",
    "        li.append(num)\n",
    "    con=0\n",
    "print(li)"
